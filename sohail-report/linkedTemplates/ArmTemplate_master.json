{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sohail-report"
		},
		"ls_adls_ccovidreportingdl_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_ccovidreportingdl'"
		},
		"ls_blob_covidreporting_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidreporting'"
		},
		"ls_blob_covidreporting1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_blob_covidreporting1'"
		},
		"ls_adls_ccovidreportingdl_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://covidreportingdlcs.dfs.core.windows.net/"
		},
		"ls_http_opendata_ecdc_data_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{linkedService().sourceBaseURL}"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sohail-report_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_ccovidreportingdl_accountKey": {
						"value": "[parameters('ls_adls_ccovidreportingdl_accountKey')]"
					},
					"ls_blob_covidreporting_connectionString": {
						"value": "[parameters('ls_blob_covidreporting_connectionString')]"
					},
					"ls_blob_covidreporting1_connectionString": {
						"value": "[parameters('ls_blob_covidreporting1_connectionString')]"
					},
					"ls_adls_ccovidreportingdl_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_ccovidreportingdl_properties_typeProperties_url')]"
					},
					"ls_http_opendata_ecdc_data_properties_typeProperties_url": {
						"value": "[parameters('ls_http_opendata_ecdc_data_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "sohail-report_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'sohail-report_ArmTemplate_0')]"
			]
		}
	]
}